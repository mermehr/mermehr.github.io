<head>
	<meta http-equiv="content-type" content="text/html; charset=utf-8">
	<meta content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0" name="viewport">
	<!-- title -->
	<%- partial('_partial/head/title.ejs') %>

	<!-- keywords,description -->
	<%- partial('_partial/head/meta.ejs') %>

	<!-- favicon -->
	<% if (theme.favicon.dark){ %>
		<%- favicon_tag(theme.favicon.dark) %>
	<% } %>

	<!-- css, script -->
	<%- css('https://unpkg.com/font-awesome@4.7.0/css/font-awesome.min.css') %>
	<%- css('css/main.css') %>

	<%- js('https://cdn.jsdelivr.net/npm/darkreader@4.9.58/darkreader.min.js') %>
	<%- js('js/main.js') %>

	<!-- custom css and js -->
	<%- partial('_partial/head/custom.ejs') %>

	<!-- search -->
	<%- partial('_partial/head/search.ejs') %>

	<!-- statistics -->
	<%- partial('_partial/head/statistics.ejs') %>

	<!-- analysis -->
	<%- partial('_partial/head/analysis.ejs') %>
</head>